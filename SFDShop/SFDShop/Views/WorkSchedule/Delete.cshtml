@model SFDShop.Application.ViewModel.WorkSchedule.NewWorkScheduleVm

<h2>Delete Work Schedule</h2>

<p>Are you sure you want to delete this schedule for employee @Model.EmployeeId?</p>

<form asp-action="DeleteConfirmed">
    <input type="hidden" asp-for="Id" />
    <button type="submit" class="btn btn-danger">Delete</button>
    <a asp-action="Index" class="btn btn-secondary">Cancel</a>
</form>
